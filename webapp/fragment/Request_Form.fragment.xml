<core:FragmentDefinition xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form"
	xmlns:ca="sap.ca.ui">
	<Dialog id="request_dialogId" title="Create Request Form" contentWidth="40%" class="dialog_title_class">
		<content>
			<VBox width="94%" class="Form_Vbox_class">
				<Label text="Client Name" class="sapUiTinyMarginBegin sapUiSmallMarginTop"/>
				<Input class="sapUiTinyMarginBegin sapUiTinyMarginEnd Form_Input_class"/>
			</VBox>
			<VBox width="94%" class="Form_Vbox_class">
				<Label text="Project Name/Assignment Name" class="sapUiTinyMarginBegin sapUiSmallMarginTop"/>
				<Input class="sapUiTinyMarginBegin sapUiTinyMarginEnd Form_Input_class"/>
			</VBox>
			<VBox width="94%" class="Form_Vbox_class">
				<Label text="Engagement Type" class="sapUiTinyMarginBegin sapUiSmallMarginTop"/>
				<Input class="sapUiTinyMarginBegin sapUiTinyMarginEnd Form_Input_class"/>
			</VBox>
			<VBox width="94%" class="Form_Vbox_class">
				<Label text="Skill Required" class="sapUiTinyMarginBegin sapUiSmallMarginTop"/>
				<Input class="sapUiTinyMarginBegin sapUiTinyMarginEnd Form_Input_class"/>
			</VBox>
			<VBox width="94%" class="Form_Vbox_class">
				<Label text="Requirement in Detail" class="sapUiTinyMarginBegin sapUiSmallMarginTop"/>
				<Input class="sapUiTinyMarginBegin sapUiTinyMarginEnd Form_Input_class"/>
			</VBox>
			<VBox width="94%" class="Form_Vbox_class">
				<Label text="Start Date of the Assignment" class="sapUiTinyMarginBegin sapUiSmallMarginTop"/>
				<DatePicker class="sapUiTinyMarginBegin sapUiTinyMarginEnd Form_Input_class"/>
			</VBox>
			<VBox width="94%" class="Form_Vbox_class">
				<Label text="Estimated Hours" class="sapUiTinyMarginBegin sapUiSmallMarginTop"/>
				<Input class="sapUiTinyMarginBegin sapUiTinyMarginEnd Form_Input_class"/>
			</VBox>
			<VBox width="94%" class="Form_Vbox_class">
				<Label text="Crave project Manager" class="sapUiTinyMarginBegin sapUiSmallMarginTop"/>
				<Input class="sapUiTinyMarginBegin sapUiTinyMarginEnd Form_Input_class"/>
			</VBox>
			<VBox width="94%" class="Form_Vbox_class">
				<Label text="Onsite/Remote" class="sapUiTinyMarginBegin sapUiSmallMarginTop"/>
				<Input class="sapUiTinyMarginBegin sapUiTinyMarginEnd Form_Input_class"/>
			</VBox>
			<VBox width="94%" class="Form_Vbox_class">
				<Label text="Remark" class="sapUiTinyMarginBegin sapUiSmallMarginTop"/>
				<Input class="sapUiTinyMarginBegin sapUiTinyMarginEnd Form_Input_class"/>
			</VBox>
			<!--<VBox width="94%" class="Form_Vbox_class">-->
			<!--	<Label text="Remark" class="sapUiTinyMarginBegin sapUiSmallMarginTop"/>-->
			<!--	<TextArea class="sapUiTinyMarginBegin sapUiTinyMarginEnd Form_TextArea_class"/>-->
			<!--</VBox>-->
		</content>
		<beginButton>
			<Button text="Save" class="update_but_class"/>
		</beginButton>
		<endButton>
			<Button text="Close" class="close_but_class" press="onDialogCloserequest"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>